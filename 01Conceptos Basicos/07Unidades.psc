//Calcular las unidades de un numero dado
Algoritmo Unidades
	//Declaracion de variables
	Definir num Como Real;
	//Inicializacion de valores
	num = 0;
	Escribir 'Ingrese un numero';
	Leer num;	//Lectura del numero
	Escribir 'Las unidades del numero son: ' num%10;	//Mensaje final
FinAlgoritmo
