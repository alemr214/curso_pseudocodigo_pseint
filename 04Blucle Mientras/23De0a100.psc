//Crear un programa que imprima todos los numeros de 0 a 100 de 1 en 1
Algoritmo De0a100
	//Declaracion de variables
	Definir num Como Entero;
	num = 0;	//Inicializacion de variables
	//Bucle
	Mientras num <= 100 Hacer
		Escribir num Sin Saltar ', ';
		num = num + 1;
	FinMientras
FinAlgoritmo
